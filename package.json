{
  "name": "agnos-code-challenge",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "reinstall": "lerna clean -y && yarn install",
    "start:dev": "lerna run --stream start:dev",
    "start:server": "yarn workspace server start:prod",
    "build": "lerna run build",
    "build:server": "yarn build:shared && yarn workspace server build",
    "build:client": "yarn build:shared && yarn workspace client build",
    "build:shared": "yarn workspace shared build"
  },
  "devDependencies": {
    "lerna": "^4.0.0"
  }
}
